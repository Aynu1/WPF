<Page x:Class="Basket.VisitorMenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Basket"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="VisitorMenuPage">

    <Grid Background="White">
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <StackPanel Orientation="Horizontal" >
                <Button x:Name="TeamsBtn" Content="Teams" Height="25" Width="60" Click="TeamsBtn_Click" />
                <Button x:Name="PlayersBtn" Content="Players" Height="25" Width="60" Click="PlayersBtn_Click"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Button x:Name="Matchups" Content="Matchups" Height="25" Width="60" Click="Matchups_Click" />
                <Button x:Name="PhotoBtn" Content="Photo" Height="25" Width="60" Click="PhotoBtn_Click"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
