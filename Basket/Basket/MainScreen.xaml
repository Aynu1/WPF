<Page x:Class="Basket.MainScreen"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Basket"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainScreen">

    <Grid Background="White">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Width="50" Content="Visiter" x:Name="VisiterBtn" Click="VisiterBtn_Click"/>
            <Button Width="50" Content="Admin" x:Name="AdminBtn" Click="AdminBtn_Click"/>
        </StackPanel>
    </Grid>
</Page>
